  "field_mappings": {
    "Image": "baseImage",
    "CommandLine": "commandLine",
    "ProcessId": "pid",
    "ParentImage": "parentBaseImage",
    "ParentCommandLine": "parentCommandLine",
    "ParentProcessId": "parentPid",
    "User": "user_username",
    "ProcessGuid": "fields[\"ProcessGuid\"]",
    "ParentProcessGuid": "fields[\"ParentProcessGuid\"]",
    "CurrentDirectory": "file_path",
    "UtcTime": "timestamp",
    "Hashes": "file_hash_sha256",
    "MD5": "file_hash_md5",
    "SHA1": "file_hash_sha1",
    "SHA256": "file_hash_sha256",
    "exe": "baseImage",
    "comm": "file_basename",
    "cmdline": "commandLine",
    "uid": "user_userId",
    "gid": "user_primaryGroupId",
    "euid": "fields[\"euid\"]",
    "egid": "fields[\"egid\"]",
    "ppid": "parentPid",
    "key": "fields[\"key\"]",
    "type": "fields[\"type\"]",
    "msg": "fields[\"msg\"]",
    "arch": "fields[\"arch\"]",
    "syscall": "fields[\"syscall\"]",
    "success": "fields[\"success\"]",
    "exit": "fields[\"exit\"]",
    "a0": "fields[\"a0\"]",
    "a1": "fields[\"a1\"]",
    "a2": "fields[\"a2\"]",
    "a3": "fields[\"a3\"]",
    "items": "fields[\"items\"]",
    "EventID": "metadata_deviceEventId",
